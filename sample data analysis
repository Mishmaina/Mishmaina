        "%%R\n",
        "ggplot(df, aes(x=Date, y=Sales)) +\n",
        "    geom_line(color=\"blue\", size=1) +\n",
        "    labs(title=\"Sales Trends Over Time\", x=\"Date\", y=\"Sales Revenue\") +\n",
        "    theme_minimal()\n"
