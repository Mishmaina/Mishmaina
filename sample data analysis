        "%%R\n",
        "# Create a sample dataset\n",
        "df <- data.frame(\n",
        "  Date = seq(as.Date(\"2023-01-01\"), as.Date(\"2023-12-31\"), by=\"month\"),\n",
        "  Sales = c(5000, 5200, 4900, 5300, 6000, 5800, 6200, 6300, 5900, 6100, 6400, 7000),\n",
        "  Category = rep(c(\"Electronics\", \"Fashion\", \"Groceries\", \"Home & Kitchen\"), each=3),\n",
        "  AgeGroup = rep(c(\"18-24\", \"25-34\", \"35-44\"), 4),\n",
        "  Gender = rep(c(\"Male\", \"Female\"), 6)\n",
        ")\n",
        "\n",
        "# View first few rows\n",
        "head(df)\n"
      ],
